                    },
                    {
                        "name": "user",
                        "in": "query",
                        "description": "The email of the user who request the job to filter on",
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "orderBy",
                        "in": "query",
                        "description": "The order of the jobs with the following syntax 'sortCriterion sortDirection' (possible values: [started|lastActivity] [asc|desc])",
